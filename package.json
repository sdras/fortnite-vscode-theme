{
  "name": "fortnite-vscode-theme",
  "displayName": "Fortnite VS Code Theme",
  "description": "A Fortnite-inspired color theme for childlike happiness even while doing serious programming",
  "version": "0.1.3",
  "publisher": "sdras",
  "author": "Sarah Drasner",
  "license": "MIT",
  "galleryBanner": {
    "color": "#370f57",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "*",
    "onCommand:fortnite.enableLegendary"
  ],
  "main": "./src/extension.js",
  "keywords": [
    "flamebows",
    "gaming",
    "play",
    "battle royal",
    "shields",
    "chickens",
    "tacos",
    "llama",
    "theme",
    "dark"
  ],
  "icon": "icon.png",
  "contributes": {
    "themes": [
      {
        "label": "Fortnite",
        "uiTheme": "vs-dark",
        "path": "./themes/fortnite-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "fortnite.enableLegendary",
        "title": "Fortnite: Enable Legendary"
      },
      {
        "command": "fortnite.disableLegendary",
        "title": "Fortnite: Disable Legendary"
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/sdras/fortnite-vscode-theme/issues",
    "email": "sarah.drasner@gmail.com"
  },
  "homepage": "https://github.com/sdras/fortnite-vscode-theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/sdras/fortnite-vscode-theme"
  }
}
